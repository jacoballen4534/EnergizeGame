<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="loadGamePane" maxHeight="900.0" maxWidth="1440.0" minHeight="768.0" minWidth="1024.0" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.loadGameController">
    <children>
        <VBox fx:id="loadGameVBox" alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" spacing="25">
            <children>
                <Label fx:id="loadGameTitle" alignment="CENTER" contentDisplay="CENTER" prefHeight="40.0" text="Load Game">
                    <font>
                        <Font size="60.0" />
                    </font>
                </Label>
                <Button fx:id="selectSaveButton" alignment="CENTER" mnemonicParsing="false" onAction="#selectSaveButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Select Save" textAlignment="CENTER" />
                <Button fx:id="loadGameBackButton" alignment="CENTER" mnemonicParsing="false" onAction="#loadGameBackButtonPressed" prefHeight="40.0" prefWidth="200.0" text="Back to Main Menu" textAlignment="CENTER" />
            </children></VBox>
    </children>
</AnchorPane>
